[ignore]
.*/build
.*/gulpfile.js
.*/node_modules/xlsx/bower.json
.*/node_modules/react-relay/lib/relayUnstableBatchedUpdates.native.js
.*/node_modules/react-tap-event-plugin/.*

[include]
.*/src
.*/tools

[libs]
./interfaces

[options]
esproposal.decorators=ignore
module.name_mapper='.+\.s?css$' -> 'CSSModule'
module.name_mapper='.+\.md$' -> 'MarkdownModule'
module.name_mapper='.+\.graphql$' -> 'GraphQLModule'
module.name_mapper='.+\.gql$' -> 'GraphQLModule'
module.system=haste
